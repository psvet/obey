    "url": "git+ssh://git@github.com/psvet/obey.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/psvet/obey/issues"
  },
  "homepage": "https://github.com/psvet/obey#readme",
  "devDependencies": {
    "jest": "^26.4.2",
    "snyk": "^1.387.0",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "dot-object": "^2.1.3",
    "jexl": "^2.1.1",
    "lodash": "^4.17.20"
  }
}
